package com.hummingbee.system;

import java.util.LinkedList;

public class SprinklerUsage {
	private LinkedList<DayUsage> usageList;
	
	
	
	/**
	 * Inner class of a single day node in the linked list
	 * @author Nick
	 *
	 */
	class DayUsage {
		private double usage;
		
		public DayUsage() {
			usage = 0;
		}
		
		public double getUsage() {
			return usage;
		}
		
		public void setUsage(double usage) {
			this.usage = usage;
		}
	}
	
}
